gg.alert('Hack By: Hikaro use com responsabilidades')


--

while( true ) do
if gg.isVisible(false) then
    menuk = 1
    gg.setVisible(false)
    open = 1
    end
--
function open()
menu = gg.choice({'⟪ Paraquedas ⟫','EXIT ✘'},nil,'Paraquedas Atualizado')
if menu == nil then exit2() end
if menu == 1 then att() end
if menu == 2 then exit() end
menuk = -1
end
--
	function att()
gg.setRanges(gg.REGION_ANONYMOUS | gg.REGION_CODE_APP)
gg.searchNumber('.8;-6::', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber('-6', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(5000)
local t = gg.getResults(5000)
for i, v in ipairs(t) do
	if v.flags == gg.TYPE_FLOAT then
		v.value = "-99996"
		v.freeze = true
    end
end
end
--
function exit()
gg.toast('FIM')
os.exit()
end
--
function exit2()
gg.toast('MINIMIZADO')
end

if menuk == 1 then 
open() 
end
end
--
