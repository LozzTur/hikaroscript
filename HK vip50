function land()
menu = gg.multiChoice({'‚ú§ Night Mode','‚ú§ Underground','‚ú§ All Female [LOBBY]üîù','‚ú§ All Male [LOBBY] üîù','‚ú§ No Fog [LOBBY]üîù','‚ú§ No Wall, No Tree e No Grass [LOBBY]üîù','‚ú§ No Land','‚ú§ FoV Agachado','‚Ü∏Back'},nil,'Modifique o mapa do game etc...')
if menu [1] == true then night() end
if menu [2] == true then under() end
if menu [3] == true then female() end
if menu [4] == true then male() end -- male
if menu [5] == true then fog() end -- fog
if menu [6] == true then tudo() end -- tudo
if menu [7] == true then no() end -- no
if menu [8] == true then fovg() end
if menu [9] == true then open() end
menuk = -1
end
